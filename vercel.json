{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
    {
      "source": "/",
      "destination": "/learn"
    }
  ],
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ]
}
