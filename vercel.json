{
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ],
  "redirect": [
    {
      "source": "/",
      "destination": "/learn",
      "permanent": true
    }
  ]
}
